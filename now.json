{
  "version": 2,
  "name": "now-next9-static",
  "regions": ["cdg"],
  "builds": [
    { "src": "public/package.json", "use": "@now/next" }
  ],
  "routes": [        
    
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": {
        "x-request-path": "$1"
      }
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/public/$1.html",
      "headers": {
        "x-request-path": "$1"
      }      
    }
    
   ]  ,
   "env":{
     "NODE_ICU_DATA":"node_modules/full-icu/"
   }

}
